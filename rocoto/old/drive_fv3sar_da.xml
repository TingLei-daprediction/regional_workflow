    </envar>
    <envar>
      <name>PDY</name>
      <value><cyclestr>@Y@m@d</cyclestr></value>
    </envar>
    <envar>
      <name>cyc</name>
      <value><cyclestr>@H</cyclestr></value>
    </envar>

    <dependency>
        <taskdep task="chgres_fcstbndy_tm06"/>
    </dependency>

  </task>

<!--  ***********************************************************************  -->
<!--  ********** Run the tm06 fcst - tm01 analysis as a metatask*************  -->
<!--  ***********************************************************************  -->

