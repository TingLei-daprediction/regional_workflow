  </metatask>

<!--  ***********************************************************************  -->
<!--  ********** Run the post jobs for tm06-tm01 *****************************  -->
<!--  ***********************************************************************  -->

  <metatask>
   <var name="mark">06 05 04 03 02 01</var>
   <metatask>
    <var name="fhr">00 01</var>
    <task name="post_f#fhr#_tm#mark#" cycledefs="regional" maxtries="6">
